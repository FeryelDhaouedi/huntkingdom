                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
<header class="header-style-2">

        <!-- .container -->
    
    <!-- .bg-header-top -->

    <!-- Start Menu -->
    <div class="bg-main-menu menu-scroll">
        <div class="container">
            <div class="row">

                <!-- .main-menu -->
            </div>
            <!-- .row -->
        </div>
        <!-- .container -->
    </div>
    <!-- .bg-main-menu -->
</header>

<!-- Start Page Header Section -->

<section class="bg-page-header">
    <div class="page-header-overlay">
        <div class="container">
            <div class="row">
                <div class="page-header">
                    <div class="page-title">
                        <h2>our blog post</h2>
                    </div>
                    <!-- .page-title -->
                    <div class="page-header-content">
                        <ol class="breadcrumb">
                            <li><a href="index.html">Home</a></li>
                            <li>Blog</li>
                        </ol>
                    </div>
                    <!-- .page-header-content -->
                </div>
                <!-- .page-header -->
            </div>
            <!-- .row -->
        </div>
        <!-- .container -->
    </div>
    <!-- .page-header-overlay -->
</section>


<!-- End Page Header Section -->


<!-- Start Blog Section -->


<section class="bg-blog-section">
    <div class="container">
        <div class="row">
            <div class="blog-section blog-page">
                <div class="row">
                    {% for blog in blogs %}
                    <div class="col-md-4 col-sm-6 col-xs-6">
                        <div class="blog-items">
                            <div class="blog-img">
                                {%if blog.type=="Image"  %}
                                    <a href="{{ path('blog_details', { 'id': blog.id }) }}"><img src="{{ asset('blog/') }}{{ blog.attachment }}"  alt="blog-img-10" class="img-responsive" /></a>
                                {% endif %}
                                {%if blog.type=="Video"  %}
                                    <video width="320" height="240" >
                                        <source src="{{ asset('blog/') }}{{ blog.attachment }}" type="video/mp4">
                                    </video>
                                {% endif %}
                            </div>
                            <!-- .blog-img -->
                            <div class="blog-content-box">
                                <div class="blog-content">
                                    <h4><a href="{{ path('blog_details', { 'id': blog.id }) }}">{{ blog.titre }}</a></h4>
                                    <p>{{ blog.description }}</p>
                                </div>
                                <!-- .blog-content -->
                                <div class="meta-box">
                                    <ul class="meta-post">
                                        <li><i class="fa fa-calendar" aria-hidden="true"></i>{% if blog.date %}{{ blog.date|date('Y-m-d H:i:s') }}{% endif %}</li>
                                        <li><a href="#"><i class="fa fa-heart-o" aria-hidden="true"></i> 24 Like</a></li>
                                        <li><a href="#"><i class="fa fa-commenting-o" aria-hidden="true"></i> 24 Comment</a></li>
                                    </ul>
                                </div>
                                <!-- .meta-box -->
                            </div>
                            <!-- .blog-content-box -->
                        </div>
                        <!-- .blog-items -->
                    </div>
                    {% endfor %}
                    <!-- .col-md-4 -->

                    <!-- .col-md-4 -->

                    <!-- .col-md-4 -->

                    <!-- .col-md-4 -->

                    <!-- .col-md-4 -->

                    <!-- .col-md-4 -->

                    <!-- .col-md-4 -->

                    <!-- .col-md-4 -->

                    <!-- .col-md-4 -->
                </div>
                <!-- .row -->
                <div class="pagination-option">
                    <nav aria-label="Page navigation">
                        <ul class="pagination">
                            <li>
                                <a href="#" aria-label="Previous">
                                    <i class="fa fa-angle-double-left" aria-hidden="true"></i>
                                </a>
                            </li>
                            <li><a href="#">1</a></li>
                            <li class="active"><a href="#">2</a></li>
                            <li><a href="#">3</a></li>
                            <li><a href="#">...</a></li>
                            <li><a href="#">5</a></li>
                            <li>
                                <a href="#" aria-label="Next">
                                    <i class="fa fa-angle-double-right" aria-hidden="true"></i>
                                </a>
                            </li>
                        </ul>
                    </nav>
                </div>
                <!-- .pagination_option -->
            </div>
            <!-- .blog-section -->
        </div>
        <!-- .container -->
    </div>
    <!-- .bg-blog-section -->
</section>


<!-- End Blog Section -->


